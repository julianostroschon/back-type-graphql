{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.packageManager": "yarn",
  "eslint.alwaysShowStatus": true,
   "eslint.validate": [
    "vue",
    "jsx",
    "tsx",
    "ts",
    "typescript",
    "html",
    "javascript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#4a39c0",
    "activityBar.background": "#4a39c0",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#cc6253",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#4a39c0",
    "statusBar.background": "#3b2d99",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#4a39c0",
    "statusBarItem.remoteBackground": "#3b2d99",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#3b2d99",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#3b2d9999",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#3b2d99"
}