{
  "name": "back-parti-notes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "run:dev": "yarn run build:dev",
    "build:dev": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build:schema": "tsnd --transpile-only src/",
    "lint:ci": "eslint -f json -o coverage/eslint.json src/",
    "format": "prettier . -w -u",
    "format:check": "prettier . -c -u",
    "lint": "eslint --max-warnings=0",
    "prepare": "husky install",
    "postinstall": "yarn husky install",
    "codegen": "graphql-codegen --config codegen.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/partithura-estagiarios/back-parti-notes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/partithura-estagiarios/back-parti-notes/issues"
  },
  "homepage": "https://github.com/partithura-estagiarios/back-parti-notes#readme",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@types/compression": "0.0.36",
    "@types/graphql": "14.0.7",
    "@types/jest": "^29.2.4",
    "@types/node": "11.9.4",
    "graphql-import-node": "0.0.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "1.18.10",
    "pg": "^8.8.0",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "8.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.191",
    "apollo-server": "^3.11.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "graphql": "^15.8.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "lint-staged": {
    "*": [
      "yarn pretty-quick --staged"
    ]
  }
}
