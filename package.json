{
  "name": "back-parti-notes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsnd --respawn src/index.ts",
    "watch": "tsc -w",
    "build:schema": "tsnd --transpile-only src/",
    "lint:ci": "eslint -f json -o coverage/eslint.json src/",
    "format": "prettier . -w -u",
    "format:check": "prettier . -c -u",
    "lint": "eslint --max-warnings=0",
    "prepare": "husky install",
    "postinstall": "yarn husky install",
    "codegen": "graphql-codegen --config codegen.ts",
    "insertUsers": "tsc ./support/faker/insertUser",
    "unit:test": "jest",
    "migrate:latest": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "seed:run": "npx knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/partithura-estagiarios/back-parti-notes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/partithura-estagiarios/back-parti-notes/issues"
  },
  "homepage": "https://github.com/partithura-estagiarios/back-parti-notes#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/compression": "0.0.36",
    "@types/graphql": "14.0.7",
    "@types/jest": "^29.2.4",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "11.9.4",
    "@types/password-hash": "^1.2.21",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.46.0",
    "apollo-server-express": "^3.11.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security-node": "^1.1.1",
    "graphql-import-node": "0.0.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "1.18.10",
    "password-hash": "^1.2.2",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "8.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.14",
    "apollo-server": "^3.11.1",
    "apollo-server-core": "^3.11.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "graphql": "^15.8.0",
    "hyperid": "^3.0.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "pino": "^8.7.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
